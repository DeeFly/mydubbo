<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	   http://code.alibabatech.com/schema/dubbo
	   http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--配置下面的东西时，id和ref是什么好像并不重要，只是有时候需要重启两次才能生效，应该是注册的缓存没有清理干净-->
    <bean class="com.gaofei.impl.RedisTestImpl" id="xxaaredisTest"/>
    <dubbo:service ref="xxaaredisTest" interface="com.gaofei.interf.RedisTest">
        <dubbo:method name="set" timeout="15000"></dubbo:method>
    </dubbo:service>
</beans>